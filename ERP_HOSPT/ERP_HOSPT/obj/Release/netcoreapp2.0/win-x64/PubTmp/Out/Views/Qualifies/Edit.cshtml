@model ERP_HOSPT.Data.Qualify



@{
    Layout = null;
}



<div class="card">
    <div class="card-body">
        <form asp-action="Edit" asp-route-id="@Model.QualifyId" onsubmit="return jQueryAjaxPost(this);">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="QualifyId" />
            <div class="form-group">
                <label asp-for="q_name" class="control-label"></label>
                <input asp-for="q_name" class="form-control" />
                <span asp-validation-for="q_name" class="text-danger"></span>
            </div>
            <br />
           
            <div class="text-center">


                <button type="submit" class="btn btn-primary col-3"> <i class="bi bi-save"></i> حفط</button>

                <button type="reset" class="btn btn-primary col-3"> <i class="bi bi-arrow-repeat me-1"></i> تحديث</button>
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
